{% extends 'base.html.twig' %}

{% block content %}

    <table class="table table-hover">
        <thead>
        <tr>
            <th>Date</th>
            <th>Type</th>
            <th>Message</th>
        </tr>
        </thead>
        <tbody>

        {% for log in logs %}
            <tr>
                <td>{{ log.0 }} {{ log.1 }}</td>
                <td>{{ log.2 }}</td>
                <td>{{ log.3 }}</td>
            </tr>
        {% endfor %}

        </tbody>
    </table>

{% endblock %}